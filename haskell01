main = do
    putStrLn "Digite dois números:"
    num1 <- readLn
    num2 <- readLn

    putStrLn $ "Soma: " ++ show (num1 + num2)
    putStrLn $ "Subtração: " ++ show (num1 - num2)
    putStrLn $ "Multiplicação: " ++ show (num1 * num2)
    if num2 /= 0
        then putStrLn $ "Divisão: " ++ show (num1 / num2)
        else putStrLn "Divisão por zero não é possível."
